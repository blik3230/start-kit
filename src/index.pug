extends layouts/l-page-index/l-page-index

block title
	title Главная страница

block content
	-
		var pages = [
		{
			title: 'Главная',
			href: 'page-home.html'
		},
		{
			title: 'Контакты',
			href: 'page-home.html'
		}
		];

	ol.list-pages
		each page in pages
			li.list-pages__item
				a.link(href=page.href, target='_blanck')= page.title

	.iframe
		iframe#iframe